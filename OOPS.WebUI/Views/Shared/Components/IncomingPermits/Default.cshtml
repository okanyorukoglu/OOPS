@model IEnumerable<OOPS.DTO.Employee.PermitDTO>


    <ul class="list-group-item">
        <li class="list-group-item  align-items-center">
            <span class="caption-subject font-dark bold "><i class="icon-umbrella"></i> Yaklaşan İzinler </span>
        </li>
        @foreach (var item in Model)
        {
        <li class="list-group-item d-flex justify-content-between align-items-center">

            <div class="row">
                <div class="col-md-12">@item.StartDate</div>
            </div>
        </li>
        }
        <li class="list-group-item d-flex justify-content-between align-items-center">

            <div class="row">
                <div class="col-md-12">Yaklaşan izin  bulunmamaktadır.</div>
            </div>
        </li>
    </ul>
