@{
    ViewData["Title"] = "Home Page";
}


<div class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <!-- Çalışan Dağılımı Bloğu -->
                <div class="col-md-4">
                    <div class="portlet light portlet-fit ">
                        <div class="portlet-title">
                            <div class="caption">
                                <i class=" icon-layers font-dark"></i>
                                <span class="caption-subject font-dark bold ">Çalışan Dağılımı</span>
                            </div>
                            <div class="actions">
                                <a class="btn btn-circle btn-icon-only btn-default" href="javascript:;">
                                    <i class="glyphicon glyphicon-option-horizontal"></i>
                                </a>
                            </div>
                        </div>
                        <div class="portlet-body">
                            <div id="donut" class="chart"> </div>
                        </div>
                        <div class="portlet-body">
                            <a href="#"><span>Raporlara Git</span></a>
                        </div>
                    </div>
                </div>
                <!------------------------>
                <!--Etkinlik Takvimi-->
                <div class="col-md-4">
                    <ul class="list-group-item">
                        <li class="list-group-item  align-items-center">
                            <span class="caption-subject font-dark bold "><i class="icon-calendar"></i> Etkinlik Takvimi </span>
                        </li>
                        <li class="list-group-item d-flex justify-content-between align-items-center">
                            Ads
                            <span class="badge badge-primary badge-pill">50</span>
                        </li>
                        <li class="list-group-item d-flex justify-content-between align-items-center">
                            Junk
                            <span class="badge badge-primary badge-pill">99</span>
                        </li>
                    </ul>
                </div>
                <!------------------------>
                <!--Yaklaşan Doğum Günleri-->
                <div class="col-md-4">
                    <ul class="list-group-item">
                        <li class="list-group-item  align-items-center">
                            <span class="caption-subject font-dark bold "> <i class="icon-present"></i> Yaklaşan Doğum Günleri </span>

                        </li>
                        <li class="list-group-item d-flex justify-content-between align-items-center">
                            Ads
                            <span class="badge badge-primary badge-pill">50</span>
                        </li>
                        <li class="list-group-item d-flex justify-content-between align-items-center">
                            Junk
                            <span class="badge badge-primary badge-pill">99</span>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="row">
                <!------------------------>
