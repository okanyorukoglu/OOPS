@model IEnumerable<OOPS.DTO.Static.VisaDocumentRequestDTO>

<ul class="list-group-item">
    <li class="list-group-item  align-items-center">
        <span class="caption-subject font-dark bold "> <i class="icon-globe"></i> Vize Belgesi Talepleri </span>

    </li>
    @for (int i = 0; i < 3; i++)
    {
        @if (Model.Count() > 0 && i < Model.Count() && Model.ToList()[i] != null)
        {
            <li class="list-group-item d-flex justify-content-between align-items-center">

                <div class="row">
                    <div class="col-md-12">@Model.ToList()[i].VisaType.VisaTypeName</div>
                </div>
            </li>
        }
    }


